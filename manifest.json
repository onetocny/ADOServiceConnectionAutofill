{
    "manifest_version": 3,
    "name": "ADO service connection autofill",
    "version": "1.3",
    "description": "todo",
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "https://dev.azure.com/*"
        ],
        "run_at": "document_idle"
      }
    ],
    "icons": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  }